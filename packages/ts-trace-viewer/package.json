{
  "name": "ts-trace-viewer",
  "displayName": "ts-trace-viewer",
  "description": "view ts perf traces ",
  "version": "0.0.1",
  "license": "MIT",
  "repository": "https://github.com/typeholes/ts-trace-viewer",
  "publisher": "typeholes",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "ts-trace-viewer",
      "properties": {
        "ts-trace-viewer.checkTimeWarning": {
          "type": "number",
          "default": 1,
          "description": "Warn when type check times exceed this number of seconds"
        }
      }
    },
    "commands": [
      {
        "command": "ts-trace-viewer.openInBrowser",
        "title": "Open trace view in browser",
        "icon": {
          "dark": "resources/todo.svg",
          "light": "resources/todo.svg"
        }
      },
      {
        "command": "ts-trace-viewer.runTrace",
        "title": "tsc trace",
        "icon": {
          "dark": "resources/todo.svg",
          "light": "resources/todo.svg"
        }
      },
      {
        "command": "ts-trace-viewer.goToType",
        "title": "goToType",
        "icon": {
          "dark": "resources/light/goto.svg",
          "light": "resources/dark/goto.svg"
        },
        "when": "view == ts-trace-viewer-view && viewItem =~ /^type:/"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile & cp -pr resources/ssr/node_modules out/",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@types/vscode": "^1.89.0",
    "axios": "^1.2.1",
    "quasar": "^2.16.0",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12",
    "zod": "^3.23.7"
  }
}